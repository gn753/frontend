{
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "requirePragma": false,
  "insertPragma": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "types/(.*)$",
    "base/(.*)$",
    "hooks/(.*)$",
    "components/(.*)$",
    "layouts/(.*)$",
    "/pages/(.*)$",
    "^[./]"
  ]
}
